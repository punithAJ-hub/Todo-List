<%- include('header'); -%>
    <div class="box" id="heading">
        <h1>
            <%= ListTittle %>
        </h1> 
    </div>
    
    <div class="box">
        <% for(var i=0;i < newItems.length ; i++){ %>
           <div class="item">
            <input type="checkbox">
               <p><%= newItems[i] %></p> 
           </div>
        <% } %>

        <form class="item" action="/addItem" method="post">
            <input type="text" name="newItem" id="newItem" placeholder="New item" autocomplete="off">
            <button type="submit" class="button" name="listType" value = <%= ListTittle %>>+</button>
        </form>

    </div>

<%- include('footer'); -%>    
   
